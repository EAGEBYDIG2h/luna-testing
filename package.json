  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "chalk": "^2.4.1",
    "ci-info": "^1.1.3",
    "escodegen": "^1.9.1",
    "esprima": "^4.0.0",
    "express": "^4.16.3",
    "istanbul-api": "^1.3.1",
    "istanbul-lib-coverage": "^1.2.0",
    "magic-string": "^0.24.0",
    "progress": "^2.0.0",
    "puppeteer": "^1.4.0",
    "rollup": "^0.58.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-replace": "^2.0.0",
    "source-map": "^0.7.3",
    "v8-to-istanbul": "^1.2.0",
